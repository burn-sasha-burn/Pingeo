{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "object-literal-sort-keys": false
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "Content/dist"
    ]
  }
}
